{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Final Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HackYourFuture-CPH/boilerplate-for-fp.git"
  },
  "main": "index.js",
  "engines": {
    "node": "12.9.1"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node ./src/server/bin/server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon ./src/server/bin/server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:clean": "cd ./src/server/ && knex migrate:rollback",
    "db:setup": "cd ./src/server/ && knex migrate:latest && knex seed:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "code:check": "npm run code:lint && npm run code:format -- --check",
    "code:clean": "npm run code:lint -- --fix && npm run code:format -- --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier \"./src/**/**.{js,jsx,ts,tsx}\"",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "knex": "knex --cwd ./src/server",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "author": "Shanawaz Islam",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-knobs": "^5.3.21",
    "app-root-path": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "circular-json": "^0.5.9",
    "clean-webpack-plugin": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "debug": "^4.3.1",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "final-form": "^4.20.1",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.4.2",
    "formik": "^1.5.8",
    "html-webpack-plugin": "^4.5.1",
    "interactjs": "^1.10.2",
    "jest-css-modules-transform": "^3.1.0",
    "jest-styled-components": "^7.0.3",
    "knex": "^0.20.15",
    "moment-timezone": "^0.5.32",
    "morgan": "^1.10.0",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-sass": "^4.14.1",
    "pg": "^7.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-social-login-buttons": "^3.1.2",
    "sass-loader": "^7.1.0",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.2.0",
    "swagger-jsdoc": "^4.3.2",
    "swagger-ui-express": "^4.1.6",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/node-logger": "^6.1.15",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.15",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "concurrently": "^4.1.0",
    "express-form-data": "^2.0.14",
    "husky": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mock-knex": "^0.4.9",
    "nodemon": "^1.12.1",
    "poetic": "^1.6.0",
    "sequelize-cli": "^5.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run code:check"
    }
  },
  "bugs": {
    "url": "https://github.com/HackYourFuture-CPH/boilerplate-for-fp/issues"
  },
  "homepage": "https://github.com/HackYourFuture-CPH/boilerplate-for-fp#readme",
  "keywords": []
}
